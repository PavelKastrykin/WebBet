<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://xmlns.jcp.org/xml/ns/javaee"
        xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
        http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1" >

    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>ChangeLocale</servlet-name>
        <servlet-class>ChangeLocale</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginWebBet</servlet-name>
        <servlet-class>LoginWebBet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ChangeLocale</servlet-name>
        <url-pattern>/changeLocale</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginWebBet</servlet-name>
        <url-pattern>/loginWebBet</url-pattern>
    </servlet-mapping>

</web-app>
